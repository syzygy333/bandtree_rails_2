<%= form_for @release do |f| %>
  <div class="field" id="unlink">
    <%= f.label :artists, "Connected artists" %>
    <%= f.collection_select :artists, @release.artists.order(id: :desc), :id, :full_name, prompt: true %>
    <%= f.submit "Unlink Artist", class: "tiny alert button" %>
  </div>
<% end %>
